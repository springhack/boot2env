{
  "name": "react-ssr-f2e",
  "version": "1.0.0",
  "description": "React SSR F2E Project",
  "main": "./lib/main.js",
  "scripts": {
    "test": "npm run build",
    "dev": "BABEL_ENV=development webpack-dev-server --progress",
    "bs": "BABEL_ENV=development mkdir -p lib && babel src/common -d lib && babel src/server -d lib",
    "bc": "BABEL_ENV=production NODE_ENV=production webpack --progress --config webpack.production.config.js",
    "build": "npm run bc && npm run bs",
    "start": "NODE_SSR=true node lib/main.js"
  },
  "keywords": [
    "React",
    "SpringHack"
  ],
  "author": "SpringHack",
  "license": "GPL-3.0",
  "dependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.26.2",
    "cssnano": "^3.10.0",
    "express": "^4.15.2",
    "express-session": "^1.15.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "mobx": "^3.1.3",
    "mobx-react": "^4.1.2",
    "multer": "^1.3.0",
    "open-browser-webpack-plugin": "^0.0.4",
    "postcss-loader": "^1.3.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-router": "^3.0.2",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
